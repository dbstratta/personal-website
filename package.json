  "scripts": {
    "start": "cross-env HTTPS=true react-app-rewired start",
    "build": "react-app-rewired build",
    "prebuild": "yarn typecheck:ts && yarn clean",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "react-app-rewired test --env=jsdom",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:ci": "yarn test:ci:unit && yarn test:ci:e2e",
    "test:ci:unit": "react-app-rewired test --env=jsdom --coverage --ci",
    "test:ci:e2e": "jest --config=jest.e2e.config.js --ci",
    "lint": "yarn lint:shell && yarn lint:md && yarn lint:js && yarn lint:ts",
    "lint:ts": "tslint --project . --fix",
    "lint:shell": "shellcheck scripts/**/*.sh",
    "lint:js": "eslint --fix .",
    "lint:md": "yarn markdownlint --ignore node_modules **/*.md",
    "lint:ci": "yarn lint:ci:commit && yarn lint:ci:shell && yarn lint:ci:md && yarn lint:ci:js && yarn format:ci && yarn lint:ci:ts",
    "lint:ci:ts": "tslint --project .",
    "lint:ci:shell": "shellcheck scripts/**/*.sh",
    "lint:ci:js": "exit 0",
    "lint:ci:md": "yarn markdownlint --ignore node_modules **/*.md",
    "lint:ci:commit": "commitlint-travis",
    "format": "prettier --write src/**/*.{tsx,ts}",
    "format:ci": "prettier --list-different src/**/*.{tsx,ts}",
    "typecheck:ts": "tsc",
    "coverage": "codecov",
    "clean": "rimraf build",
    "commit": "commit"
  },
