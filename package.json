  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "prebuild:prod": "yarn typecheck",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:unit:watch": "yarn test:unit --onlyChanged --watch",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:ci": "yarn test:ci:unit",
    "test:ci:unit": "yarn test:unit --ci --coverage",
    "test:ci:e2e": "yarn test:e2e --ci",
    "lint": "yarn lint:shell && yarn lint:md && yarn lint:ts",
    "lint:ts": "tslint --project . --fix && eslint --fix .",
    "lint:shell": "shellcheck scripts/**/*.sh",
    "lint:md": "yarn markdownlint --ignore node_modules **/*.md",
    "lint:ci": "yarn lint:ci:commit && yarn lint:ci:shell && yarn lint:ci:md && yarn format:ci && yarn lint:ci:ts",
    "lint:ci:ts": "tslint --project . && eslint .",
    "lint:ci:shell": "shellcheck scripts/**/*.sh",
    "lint:ci:md": "yarn markdownlint --ignore node_modules **/*.md",
    "lint:ci:commit": "commitlint-travis",
    "format": "prettier --write src/**/*.{tsx,ts,js}",
    "format:ci": "prettier --list-different src/**/*.{tsx,ts,js}",
    "typecheck": "yarn typecheck:ts",
    "typecheck:ts": "tsc",
    "coverage": "codecov",
    "commit": "commit"
  },
